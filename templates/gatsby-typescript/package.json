{
  "name": "gatsby-typescript-template",
  "version": "1.0.0",
  "author": {
    "name": "Digital Vision CZ",
    "url": "https://digitalvision.cz"
  },
  "private": true,
  "scripts": {
    "start": "yarn dev",
