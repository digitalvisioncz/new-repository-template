    "gatsby-plugin-typescript": "^5.5.0",
    "gatsby-source-filesystem": "^5.10.0",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.1",
    "postcss-nested": "^5.0.5",
    "postcss-preset-env": "^8.3.2",
    "stylelint": "13.12.0",
    "typescript": "^4.9.5"
