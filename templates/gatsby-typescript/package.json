    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@dvdevcz/eslint-config-react": "^0.0.6",
    "@dvdevcz/eslint-config-typescript": "^0.0.4",
    "@dvdevcz/stylelint-config": "^0.0.3",
    "@dvdevcz/typescript-config": "^0.0.3",
    "@graphql-eslint/eslint-plugin": "^3.10.4",
    "@types/node": "^17.0.35",
